@using SimpleApp.Models
@model IEnumerable<Product>
@{ Layout = null; }
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Simple App</title>
</head>
<body>
    <ul>
        @foreach (Product p in Model)
        {
            <li>@nameof(p.Name): @p.Name, @nameof(p.Price): @p.Price</li>
        }
    </ul>
</body>
</html>